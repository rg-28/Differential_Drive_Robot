<robot name="test_robot">

  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.16 0.16 0.12"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.085"/>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
	<box size="0.16 0.16 0.12"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2"/>
      <inertia ixx="0.006" ixy="0.0" ixz="0.0" iyy="0.006" iyz="0.0" izz="0.00853"/>
    </inertial>
  </link>
  
  <link name="lwheel">
    <visual>
      <geometry>
        <cylinder length="0.008" radius="0.046"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
	<cylinder length="0.008" radius="0.046"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.05"/>
      <inertia ixx="0.0001603" ixy="0.0" ixz="0.0" iyy="0.0001603" iyz="0.0" izz="0.0000529"/>
    </inertial> 
  </link>
  
  <joint name="base_to_lwheel" type="continuous">
    <axis rpy="0 0 0" xyz="0 0 -1"/>
    <parent link="base_link"/>
    <child link="lwheel"/>
    <origin xyz="0 0.115 0.046" rpy="1.5708 0 0"/>
  </joint>

  <transmission name="base_to_lwheel_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="base_to_lwheel_motor">
       <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="base_to_lwheel">
       <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <gazebo reference="lwheel">
    <mu1 value="200.0"/>
    <mu2 value="100.0"/>
    <kp value="10000000.0" />
    <kd value="1.0" />
    <material>Gazebo/Black</material>
  </gazebo>
  
  <link name="rwheel">
    <visual>
      <geometry>
        <cylinder length="0.008" radius="0.046"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
	<cylinder length="0.008" radius="0.046"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.05"/>
      <inertia ixx="0.0001603" ixy="0.0" ixz="0.0" iyy="0.0001603" iyz="0.0" izz="0.0000529"/>
    </inertial>
  </link>
  
  <joint name="base_to_rwheel" type="continuous">
    <axis rpy="0 0 0" xyz="0 0 1"/>
    <parent link="base_link"/>
    <child link="rwheel"/>
    <origin xyz="0 -0.115 0.046" rpy="-1.5708 0 0"/>
  </joint>

  <transmission name="base_to_rwheel_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="base_to_rwheel_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="base_to_rwheel">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <gazebo reference="rwheel">
    <mu1 value="200.0"/>
    <mu2 value="100.0"/>
    <kp value="10000000.0" />
    <kd value="1.0" />
    <material>Gazebo/Black</material>
  </gazebo>  

  <link name="fwheel_attach">
    <visual>
      <geometry>
        <cylinder length="0.060" radius="0.023"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.13 0 0.08"/>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
	<cylinder length="0.060" radius="0.023"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.05"/>
      <inertia ixx="0.00001296" ixy="0.0" ixz="0.0" iyy="0.00001296" iyz="0.0" izz="0.00001587"/>
    </inertial>
  </link>
  
  <joint name="base_to_fattach" type="fixed">
    <parent link="base_link"/>
    <child link="fwheel_attach"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>
  
  <link name="fwheel">
    <visual>
      <geometry>
        <sphere radius="0.020"/>
      </geometry>
      <origin rpy="1.578 0 0" xyz="0 0 0"/>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
	<sphere radius="0.020"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.05"/>
      <inertia ixx="0.000008" ixy="0.0" ixz="0.0" iyy="0.0000008" iyz="0.0" izz="0.000008"/>
    </inertial>
  </link>
  
  <joint name="base_to_fwheel" type="continuous">
    <axis rpy="0 0 0" xyz="1 1 1"/>
    <parent link="base_link"/>
    <child link="fwheel"/>
    <origin xyz="0.13 0 0.02" rpy="0 0 0"/>
  </joint>

  <transmission name="base_to_fwheel_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="basae_to_fwheel_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="base_to_fwheel">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <gazebo reference="fwheel">
    <mu1 value="200.0"/>
    <mu2 value="100.0"/>
    <kp value="10000000.0" />
    <kd value="1.0" />
    <material>Gazebo/Black</material>
  </gazebo>
  
  <link name="scanner">
    <visual>
      <geometry>
        <box size="0.015 0.045 0.015"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.022 0 0.015"/>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
	<box size="0.015 0.045 0.015"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.03"/>
      <inertia ixx="0.000005625" ixy="0.0" ixz="0.0" iyy="0.000001125" iyz="0.0" izz="0.000005625"/>
    </inertial>
  </link>
  
  <joint name="base_to_scanner" type="fixed">
    <parent link="base_link"/>
    <child link="scanner"/>
    <origin xyz="0.105 0 0.22" rpy="0 0 0"/>
  </joint>

  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>/</robotNamespace>
    </plugin>
  </gazebo>  
</robot>
